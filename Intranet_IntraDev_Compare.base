filters:
  or:
    - file.folder.containsAny("fahrra")
views:
  - type: table
    name: Table
    order:
      - file.name
      - file.basename
      - file.mtime
      - guid_hash
    sort:
      - property: guid_hash
        direction: ASC
    columnSize:
      file.basename: 263
      file.mtime: 208
