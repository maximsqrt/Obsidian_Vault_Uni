filters:
  or:
    - file.hasTag("status/http-4xx")
    - file.hasTag("status/http-5xx")
formulas:
  Untitled: |2
     - type == "api-endpoint"
     - file.hasTag("app/intranet")
     - file.hasTag("env/prod")
views:
  - type: table
    name: Table
    order:
      - file.name
      - rest
      - file.ctime
      - base_url
      - last_checked
      - file.folder
    sort: []
